{
    "manifest_version": 2,
    "name": "Tab Manager",
    "version": "0.1",
    "description": "Helps me manage my tabs",
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": []   
        }
    ],
    "background": {
        "scripts": ["background.js"]
      },
    "permissions": ["tabs"]
}